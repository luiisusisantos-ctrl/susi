public class susi7
{
   public static void main(String [] args){
       int [] arr = {-3,5,-7,2,-1};
       
       for (int i = 0; i < arr.length; i++){
           if (arr[i] < 0)arr[i] = -arr[i];
           }
       
       for(int x : arr) System.out.print(x + " ");
   }
}
